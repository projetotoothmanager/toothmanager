<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/main.css">
    <link rel="stylesheet" href="/css/agenda.css">
    <script src="https://kit.fontawesome.com/7491f3e6c6.js" crossorigin="anonymous"></script>
    <title>Toothmanager</title>
</head>
<body>
    <!-- button-menu-hamburguer -->
    <input type="checkbox" id="check">
    <label for="check" class="menu-hamburgue">
        <i class="fas fa-bars" id="btn"></i>
        <i class="fas fa-times" id="cancel"></i>
    </label>

    <!-- sidebar -->
    <%- include('./partials/sidebar')%>

    <!-- <section> coloque seu codigo dentro desta tag -->
    <section>
        <!-- BUSCA -->
        <div class="search-container">
            <div class="title-agenda">
                <h1>Agenda</h1>
            </div>
    
    
          <form action="#" method="get">
            <input type="text" placeholder="Buscar.." name="search">
            <button type="submit"><i class="fa fa-search"></i></button>
          </form>
        </div>
        <!-- AGENDA -->
    
        <div class="container-agenda">
            <form action="#" method="post">
                <label for="hora" name="hora">Hora:</label>
                <input type="text" id="hora">
                <label for="data" name="data">Data:</label>
                <input type="text" id="data">
                <label for="nome" name="nome">Nome:</label>
                <input type="text" id="nome">
                <button type="submit" id="btn-agenda-submit">Adicionar</button>
            </form>
            <br>
    
            <div class="cliente">
                <label for="">Hora:</label>
                <span>08:00</span>
                <label for="">Data:</label>
                <span>01/02/2022</span>
                <label for="cliente" name="cliente" id="cliente">Nome:</label>
                <span>Wictor Hiago</span>
                <button type="button" id="btn-deletar-cliente">Deletar</button>
                <button type="button" id="btn-edit-cliente">Editar</button>
            </div>
            <br>
        </div>

    </section>

</body>
</html>